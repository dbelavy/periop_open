!!!
%html
  %head
    = render 'layouts/inspectlet'
    %meta{:charset => "utf-8"}
    %meta{"http-equiv" => "X-UA-Compatible", :content => "IE=edge,chrome=1"}
    %meta{:name => "viewport", :content => "width=device-width, initial-scale=1, maximum-scale=1"}
    %meta{:name => "c923fbd2f5592a094045a0867ff98cfc29534a83", :content => "ddf419e5f9f65b40885664eb7047140f78f51310"}
    %title= content_for?(:title) ? yield(:title) : "Pre-op.net"
    %meta{:content => "", :name => "description"}
    %meta{:content => "", :name => "author"}
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
    = yield(:head)
    = render 'layouts/google_analitics'
  %body
    %header.navbar.navbar-fixed-top
      %nav.navbar-inner
        .container
          = render 'layouts/navigation'
    #main{:role => "main"}
      .container
        .content{:class => yield(:content_class)}
          .row
            .span12
              = render 'layouts/messages'
              = yield
              = debug(params) if Rails.env.development?
          %footer

